<div id="wrapper">
       <!-- Navigation -->
       <nav class="navbar navbar-default navbar-static-top" role="navigation" style="margin-bottom: 0">
           <div class="navbar-header">
               <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                   <span class="sr-only">Toggle navigation</span>
                   <span class="icon-bar"></span>
                   <span class="icon-bar"></span>
                   <span class="icon-bar"></span>
               </button>
               <%= link_to "Eltab Project", root_path, {:class => "navbar-brand"} %>
           </div>
           <!-- /.navbar-header -->
           <%= render 'layouts/header_with_menu' %>
           <%= render 'layouts/sidebar_menu' %>
           <!-- /.navbar-top-links -->
           <!-- /.navbar-static-side -->
       </nav>

       <div id="page-wrapper">
           <div class="row">
               <div class="col-lg-12">
                   <h1 class="page-header">Your patients</h1>
               </div>
               <!-- /.col-lg-12 -->
           </div>
           <!-- /.row -->
           <div class="row">
               <div class="col-lg-12">
                   <div class="panel panel-default">
                       <div class="panel-heading">
                        List shows your patients you "follow"
                       </div>
                       <!-- /.panel-heading -->
                       <div class="panel-body">
                           <div class="table-responsive">
                               <table class="table table-hover">
                                   <thead>
                                       <tr>
                                           <!-- <th>Device Id</th> -->
                                           <th>Patient Name</th>
                                           <th>Action</th>
                                       </tr>
                                   </thead>
                                   <tbody>
                                     <% @patients.all.each do |patient| %>
                                         <tr>
                                           <td><%=find_patient_username(patient.patient_id)%></tb>
                                           <td><%=link_to "Patient Profile", patient_path(patient.patient_id) %></td>
                                          </tr>
                                        <% end %>
                                   </tbody>
                               </table>
                           </div>
                           <!-- /.table-responsive -->
                       </div>
                       <!-- /.panel-body -->
                   </div>
                   <!-- /.panel -->
               </div>
             </div>
           <!-- /.row -->
           <!-- /.row -->

       <!-- /#page-wrapper -->
</div>
</div>
